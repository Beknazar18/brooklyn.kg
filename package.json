  "name": "brooklyn.kg",
  "version": "1.0.0",
  "description": "Brooklyn Bridge Site",
  "author": "Evald Smalyakov",
  "private": true,
  "scripts": {
    "serve:dev": "npm run dev",
    "dev": "nodemon --watch app --watch bootstrap --watch config --watch start --watch .env -x node server.js",
