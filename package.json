    "precommit": "npm run lint"
  },
  "standard": {
    "global": [
      "use",
      "make"
    ]
  },
  "autoload": {
    "App": "./app",
    "MODELS": "./app/Models",
    "AUTHS": "./app/Models/Auths",
    "SETTINGS": "./app/Models/Settings"
